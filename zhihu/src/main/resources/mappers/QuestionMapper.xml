<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.edu.huas.zhihu.mapper.QuestionMapper">

    <resultMap id="BaseResultMap" type="cn.edu.huas.zhihu.pojo.Question">
            <id property="id" column="id" jdbcType="BIGINT"/>
            <result property="question_text" column="question_text" jdbcType="VARCHAR"/>
            <result property="user_id" column="user_id" jdbcType="BIGINT"/>
    </resultMap>

    <sql id="Base_Column_List">
        id,question_text,user_id
    </sql>
</mapper>
